define ClearCrk_min {select flow
                    from   clear_ck_min
                    where  month=month,wytype=wyt_SHASTA_cvp}

define minflow_C_Wkytn {alias ClearCrk_min kind 'flow-min-required' units 'cfs'}  

goal meet_clearmin {C_Wkytn_MIF < ClearCrk_min}  
